{
  "name": "Giphy_Demo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "plugins": ["prettier"],
    "extends": ["plugin:prettier/recommended"],
